{
    "name": "duronrulez/ginger-php",
    "description": "The Ginger Payments PHP 5.3 SDK",
    "keywords": ["payment", "pay", "merchant", "e-commerce"],
    "license": "MIT",
    "homepage": "https://github.com/duronrulez/ginger-php",
    "authors": [
        {
            "name": "Hristo Mitev",
            "email": "duronrulez@gmail.com"
        },
        {
            "name": "Ginger Payments",
            "email": "dev@gingerpayments.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "guzzlehttp/guzzle": "~3.8.1",
        "beberlei/assert": "~2.3",
        "ronanguilloux/isocodes": "~1.1",
        "rhumsaa/uuid": "~2.8",
        "nesbot/carbon": "~1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*",
        "squizlabs/php_codesniffer": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "GingerPayments\\Payment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GingerPayments\\Payment\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
